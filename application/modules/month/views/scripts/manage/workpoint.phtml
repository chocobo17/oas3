<script type="text/javascript">
	$(function(){
		$("#proc-insert").click(function(){
			$.ajax({
				url:URL+'month/workpoint/process',
				type:'POST',
				data:decodeURI($("form[name='work-from-insert']").serialize()),
				dataType:'json',
				beforeSend:function(XHR){
					$("#proc-insert").css("disabled","disabled");
				},
				success:function(data){
					$("#nowDisplay").text(data.display);
					$("#runtimeMsg").html("&lt;span class='em_msg'&gt;"+data.msg+"&lt;/span&gt;").fadeOut(1500, function(){
						$("#runtimeMsg &gt; span").remove();
						$("#runtimeMsg").fadeIn('fast', function(){
							window.location=URL+'month/index/workpoint';
						});
					});	
					
				},
				error:function(XHR){
					alert(XHR.status);
				},
				complete:function(XHR){
					$("#proc-insert").css("disabled","");
				}
			});
		});
	});

	function remove_empty_table()
	{
		$.each($("table[class^='table-']"), function(e){
			var tr_amount = $(this).children().children().size();
			if(tr_amount == 1){ //只有標題列
				$(this).parent().parent().css('display','none');
				//不用remove是為了設定編輯月份，更新Table
			}
		});
	}

function adquery_changeDivContent(data){
